<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/unoFragment">

    <fragment
        android:id="@+id/unoFragment"
        android:name="com.example.cumpleaosonboard.UnoFragment"
        android:label="fragment_uno"
        tools:layout="@layout/fragment_uno" >
        <action
            android:id="@+id/action_unoFragment_to_dosFragment"
            app:destination="@id/dosFragment"
            app:enterAnim="@android:anim/fade_in"
            app:exitAnim="@android:anim/fade_in"
            app:popEnterAnim="@android:anim/fade_in"
            app:popExitAnim="@android:anim/fade_in" />
        <action
            android:id="@+id/action_unoFragment_to_cuatroFragment"
            app:destination="@id/cuatroFragment"
            app:enterAnim="@android:anim/fade_in"
            app:exitAnim="@android:anim/fade_in"
            app:popEnterAnim="@android:anim/fade_in"
            app:popExitAnim="@android:anim/fade_in"/>
    </fragment>
    <fragment
        android:id="@+id/dosFragment"
        android:name="com.example.cumpleaosonboard.DosFragment"
        android:label="fragment_dos"
        tools:layout="@layout/fragment_dos" >
        <action
            android:id="@+id/action_dosFragment_to_tresFragment"
            app:destination="@id/tresFragment"
            app:enterAnim="@android:anim/fade_in"
            app:exitAnim="@android:anim/fade_in"
            app:popEnterAnim="@android:anim/fade_in"
            app:popExitAnim="@android:anim/fade_in"/>
        <action
            android:id="@+id/action_dosFragment_to_cuatroFragment"
            app:destination="@id/cuatroFragment"
            app:enterAnim="@android:anim/fade_in"
            app:exitAnim="@android:anim/fade_in"
            app:popEnterAnim="@android:anim/fade_in"
            app:popExitAnim="@android:anim/fade_in"/>
    </fragment>
    <fragment
        android:id="@+id/tresFragment"
        android:name="com.example.cumpleaosonboard.TresFragment"
        android:label="fragment_tres"
        tools:layout="@layout/fragment_tres" >
        <action
            android:id="@+id/action_tresFragment_to_cuatroFragment"
            app:destination="@id/cuatroFragment"
            app:enterAnim="@android:anim/fade_in"
            app:exitAnim="@android:anim/fade_in"
            app:popEnterAnim="@android:anim/fade_in"
            app:popExitAnim="@android:anim/fade_in"/>
    </fragment>
    <fragment
        android:id="@+id/cuatroFragment"
        android:name="com.example.cumpleaosonboard.CuatroFragment"
        android:label="fragment_cuatro"
        tools:layout="@layout/fragment_cuatro" >
        <action
            android:id="@+id/action_cuatroFragment_to_cincoFragment"
            app:destination="@id/cincoFragment"
            app:enterAnim="@android:anim/fade_in"
            app:exitAnim="@android:anim/fade_in"
            app:popEnterAnim="@android:anim/fade_in"
            app:popExitAnim="@android:anim/fade_in"/>
    </fragment>
    <fragment
        android:id="@+id/cincoFragment"
        android:name="com.example.cumpleaosonboard.CincoFragment"
        android:label="fragment_cinco"
        tools:layout="@layout/fragment_cinco" >
        <action
            android:id="@+id/action_cincoFragment_to_cuatroFragment"
            app:destination="@id/cuatroFragment" />
    </fragment>
</navigation>